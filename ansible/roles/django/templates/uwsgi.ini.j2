# {{ ansible_managed }}

[uwsgi]
chdir           = {{ project_dir }}
module          = {{ app_name }}.wsgi
home            = {{ virtualenv_dir }}
logto           = {{ application_log_dir }}/uwsgi.log

master          = true
processes       = 10
socket          = {{ uwsgi_socket_path }}
chmod-socket    = 664
uid             = {{ nginx_user }}
gid             = {{ nginx_user }}
vacuum          = true
